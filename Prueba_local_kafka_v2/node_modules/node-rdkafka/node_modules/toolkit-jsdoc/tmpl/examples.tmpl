<?js
    var data = obj;
    var self = this;
    var num = 0;
    var key = Math.floor(Math.random() * 1000);

    data.forEach(function(example) {
      num++;
      ?>
      <p>
        <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#modal-example-<?js= key ?>-<?js= num ?>">
          Open Example <?js= num ?>
        </button>
      </p>
      <div class="modal hide" id="modal-example-<?js= key ?>-<?js= num ?>">
        <div class="modal-header">
          <a href="#" class="close" data-dismiss="modal" onclick="return false;">
            <i class="icon-remove icon-white"></i></a>
          <h1>Example <?js= num ?></h1>
        </div>
        <div class="modal-body">
          <pre class="prettyprint"><code><?js= self.htmlsafe(example.code) ?></code></pre>

        </div>
        <?js if (example.caption) { ?>
        <div class="modal-footer">
          <p><?js= example.caption ?></p>
        </div>
      <?js } ?>
      </div>
<?js }); ?>
